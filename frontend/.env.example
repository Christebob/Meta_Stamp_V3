# META-STAMP V3 Frontend Environment Configuration
# Copy this file to .env and update with your actual values
# Variables must be prefixed with VITE_ to be exposed to the frontend

# =============================================================================
# Backend API Configuration
# =============================================================================
VITE_API_URL=http://localhost:8000

# =============================================================================
# Auth0 Configuration (must match backend Auth0 settings)
# =============================================================================
VITE_AUTH0_DOMAIN=your-tenant.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id
VITE_AUTH0_AUDIENCE=https://api.metastamp.com

# =============================================================================
# Feature Flags
# =============================================================================
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_DARK_MODE=true

# =============================================================================
# Upload Configuration
# =============================================================================
VITE_MAX_UPLOAD_SIZE_MB=500
VITE_DIRECT_UPLOAD_THRESHOLD_MB=10

# =============================================================================
# Environment
# =============================================================================
VITE_APP_ENV=development
