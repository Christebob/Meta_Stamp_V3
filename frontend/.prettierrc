  "printWidth": 80,
  "proseWrap": "preserve",
  "singleQuote": true,
  "quoteProps": "as-needed",
  "semi": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "endOfLine": "lf"
